{
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/",
      "destination": "/api",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api",
      "destination": "/api/entrypoint.go"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/entrypoint.go"
    }
  ]
}